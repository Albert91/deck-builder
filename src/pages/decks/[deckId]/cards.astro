---
import Layout from '@/layouts/Layout.astro';
import DeckCardsPage from '@/components/decks/cards/pages/DeckCardsListPage';

const { deckId } = Astro.params;
if (!deckId) {
  return Astro.redirect('/decks');
}
---

<Layout title="Deck Cards">
  <DeckCardsPage client:load deckId={deckId} />
</Layout>
